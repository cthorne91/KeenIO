{
    "name": "sitruc/keenio",
    "description": "A convenient wrapper around the Keen.io API for Laravel",
    "keywords": [
        "keen.io",
        "keen-io",
        "keenio",
        "keen",
        "laravel-keen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Curtis Thorne",
            "email": "cthorne@me.com"
        }
    ], 
    "require": {
        "php": "^7.0",
        "keen-io/keen-io" : "~2.5",
        "illuminate/support": "5.3.*|5.4.*",  
        "illuminate/filesystem": "5.3.*|5.4.*",
        "illuminate/config": "5.3.*|5.4.*",
        "illuminate/contracts": "5.3.*|5.4.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Sitruc\\KeenIO\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sitruc\\KeenIO\\Test\\": "tests",
            "Illuminate\\": "vendor/laravel/framework/src/Illuminate"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "prefer_stable": true
}

